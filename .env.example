# Project-Specific Environment Variables for Twickenham Events
# Copy


# Enable/disable MQTT integration (true/false)
MQTT_ENABLED=MQTT_ENABLED
# MQTT broker hostname or IP
MQTT_BROKER_URL=MQTT_BROKER_URL
# MQTT broker port (usually 1883 or 8883 for TLS)
MQTT_BROKER_PORT=MQTT_BROKER_PORT
# MQTT username for authentication
MQTT_USERNAME=MQTT_USERNAME
# MQTT password for authentication
MQTT_PASSWORD=MQTT_PASSWORD
# MQTT security mode (e.g., username, none)
MQTT_SECURITY=MQTT_SECURITY
# Main MQTT client ID for this project (used by service)
MQTT_CLIENT_ID=MQTT_CLIENT_ID
# Twickenham Events-specific client ID; Randomly set by code if not defined
MQTT_CLIENT_ID_TWICKENHAM=MQTT_CLIENT_ID_TWICKENHAM
# Enable TLS for MQTT connection (true/false)
MQTT_USE_TLS=MQTT_USE_TLS
# Enable/disable TLS certificate verification (true/false)
TLS_VERIFY=TLS_VERIFY


# Path to CA certificate for TLS (optional)
TLS_CA_CERT=TLS_CA_CERT
# Path to client certificate for TLS (optional)
TLS_CLIENT_CERT=TLS_CLIENT_CERT
# Path to client key for TLS (optional)
TLS_CLIENT_KEY=TLS_CLIENT_KEY


# Enable Home Assistant integration (true/false)
HOME_ASSISTANT_ENABLED=HOME_ASSISTANT_ENABLED


# Enable ICS calendar export (true/false)
CALENDAR_ENABLED=CALENDAR_ENABLED
# Override calendar URL for external access (optional)
CALENDAR_URL_OVERRIDE=CALENDAR_URL_OVERRIDE


# Google Gemini API key for AI features
GEMINI_API_KEY=GEMINI_API_KEY


# Enable AI type detection (true/false)
TYPE_DETECTION_ENABLED=TYPE_DETECTION_ENABLED
# Enable AI event name shortening (true/false)
SHORTENING_ENABLED=SHORTENING_ENABLED
# Enable flags for AI shortening (true/false)
FLAGS_ENABLED=FLAGS_ENABLED
